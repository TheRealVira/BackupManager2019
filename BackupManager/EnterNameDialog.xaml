<Window x:Class="BackupManager.EnterNameDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BackupManager"
        mc:Ignorable="d"
        Title="EnterNameDialog" Height="124.086" Width="336.545" WindowStyle="ToolWindow" ResizeMode="NoResize">
    <StackPanel>
        <TextBlock Text="Enter a name for your backup.." />
        <TextBox x:Name="ResponseTextBox" TextChanged="ResponseTextBox_TextChanged" />
        <Button Name="btn_ok" Visibility="Hidden" Content="OK" Click="OKButton_Click" />
        <Button Content="Cancel" Click="CancelButton_Click" />
    </StackPanel>
</Window>
